# AI Configuration Environment Variables

# Enable/Disable AI Features
AI_ENABLED=false

# AI Provider (openai, gemini, or leave empty)
AI_PROVIDER=

# OpenAI Configuration
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=1000
OPENAI_TEMPERATURE=0.7

# Google Gemini Configuration
GEMINI_API_KEY=
GEMINI_MODEL=gemini-pro

# AI Logging
AI_LOG_REQUESTS=true
AI_LOG_ERRORS=true
AI_LOG_FEEDBACK=true
